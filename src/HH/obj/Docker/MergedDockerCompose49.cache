name: dockercompose11862316663359652191
services:
  hh.api:
    build:
      context: C:\Users\User\Downloads\Telegram Desktop\HH\HH
      dockerfile: HH.API/Dockerfile
    depends_on:
      postgres_image:
        condition: service_started
        required: true
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTP_PORTS: "8080"
      ASPNETCORE_HTTPS_PORTS: "8081"
      DB_CONNECTION_STRING: Host=postgres_image;Port=5432;Database=HH;User ID=postgres;Password=123;Include Error Detail=true
    image: hhapi
    networks:
      aspnetcoreapp-network: null
    ports:
      - mode: ingress
        target: 8080
        published: "8080"
        protocol: tcp
      - mode: ingress
        target: 8081
        published: "8081"
        protocol: tcp
      - mode: ingress
        target: 8080
        protocol: tcp
      - mode: ingress
        target: 8081
        protocol: tcp
    volumes:
      - type: bind
        source: C:\Users\User\AppData\Roaming/Microsoft/UserSecrets
        target: /home/app/.microsoft/usersecrets
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\User\AppData\Roaming/ASP.NET/Https
        target: /home/app/.aspnet/https
        read_only: true
        bind:
          create_host_path: true
  minio:
    command:
      - server
      - /data
      - --console-address
      - :9001
    container_name: minio
    environment:
      MINIO_ROOT_PASSWORD: minioadmin
      MINIO_ROOT_USER: minioadmin
    image: minio/minio
    networks:
      aspnetcoreapp-network: null
    ports:
      - mode: ingress
        target: 9000
        published: "9000"
        protocol: tcp
      - mode: ingress
        target: 9001
        published: "9001"
        protocol: tcp
    volumes:
      - type: volume
        source: minio-data
        target: /data
        volume: {}
  postgres_image:
    container_name: postgres_image
    environment:
      POSTGRES_DB: HH
      POSTGRES_PASSWORD: "123"
      POSTGRES_USER: postgres
    image: postgres:latest
    networks:
      aspnetcoreapp-network: null
    ports:
      - mode: ingress
        target: 5432
        published: "5432"
        protocol: tcp
    restart: always
networks:
  aspnetcoreapp-network:
    name: dockercompose11862316663359652191_aspnetcoreapp-network
volumes:
  minio-data:
    name: dockercompose11862316663359652191_minio-data